right_area <- right_area + (1 - left_prop) * left_area
left_area <- left_area * left_prop * (1 - eps)
last_cutoff <- new_cutoff
}
return (cutoff_points)
}
# algorithm 4 ----
find_cutoff_under_fixed_number <- function(v, b, n) {
l <- 0
r <- 1
while (r - l > 1e-6) {
m = (r + l) / 2
n_m = length(find_cutoff_under_rej_rate(v, b, m))
if (n_m < n) {r = m}
else {l = m}
}
res <- find_cutoff_under_rej_rate(v, b, l)
return (res)
}
# algorithm 5 ----
new_GIG = function(N, lambda, psi, chi, eps = 0.5, K = NULL) {
flag = FALSE
if(lambda > 0) {
lambda = -lambda
.psi = psi
psi = chi
chi = .psi
flag = TRUE
}
# re-parameterization
alpha = sqrt(psi / chi)
beta = sqrt(psi * chi)
# number of cutoff points
if(is.null(K)) {
cutpoint = find_cutoff_under_rej_rate(lambda, beta, eps)
} else {
cutpoint = find_cutoff_under_fixed_number(lambda, beta, K)
}
# sample from y
Y = rejection.sampling(N, lambda, beta, cutpoint)
# sample from x|y
X = sapply(seq_len(N), function(i) {
trunc_gamma(1, -lambda, beta/2, 1/Y[i])
})
# GIG variate
X = 1 / (alpha * X)
if(flag) {
X = 1 / X
}
# return
attr(X, "rej.const") = attributes(Y)[["rej.const"]]
return(X)
}
